{
  "fileTypes": ["svelte"],
  "injectionSelector": "L:source.svelte meta.lang.jazz.svelte",
  "patterns": [
    {
      "begin": "(?<=>)(?!</)",
      "end": "(?=</)",
      "name": "meta.embedded.block.svelte",
      "contentName": "source.css.jazz",
      "patterns": [
        {
          "include": "source.css.jazz"
        }
      ]
    }
  ],
  "scopeName": "source.jazz-svelte"
}
