{
  "name": "jazzcss",
  "version": "0.1.1",
  "main": "lib/Processor.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jquense/jazz.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "build": "yarn parser && 4c build src",
    "format": "4c format src './*'",
    "lint": "yarn parser && 4c lint src './*'",
    "parser": "node parsers/build.js",
    "publish-local": "yalc publish --push",
    "prepublishOnly": "yarn run build",
    "release": "4c release",
    "tdd": "jest --watch",
    "test": "yarn lint && jest && yarn typecheck",
    "testonly": "jest",
    "typecheck": "tsc --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": [
      "yarn 4c lint --fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setupAfterEnv.ts"
    ]
  },
  "devDependencies": {
    "@4c/cli": "^2.1.12",
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@types/jest": "^26.0.15",
    "@types/node": "14.14.6",
    "@types/resolve": "^1.17.1",
    "dart-sass": "^1.25.0",
    "dedent": "^0.7.0",
    "eslint": "^7.12.1",
    "jest": "^26.2.2",
    "jest-diff": "^26.2.0",
    "memfs": "^3.2.0",
    "pegjs": "^0.10.0",
    "pegjs-backtrace": "^0.2.0",
    "prettier": "^2.1.2",
    "ts-pegjs": "^0.2.7",
    "typescript": "^4.0.5",
    "yalc": "^1.0.0-pre.45"
  },
  "dependencies": {
    "@babel/node": "^7.12.6",
    "@csstools/convert-colors": "^2.0.0",
    "@modular-css/processor": "^25.8.2",
    "@modular-css/shortnames": "^25.0.0",
    "@types/color-convert": "^2.0.0",
    "@types/cssesc": "^3.0.0",
    "@types/d3-color": "^1.2.2",
    "@types/lodash": "^4.14.165",
    "acorn": "^8.0.4",
    "browserslist": "^4.14.7",
    "caniuse-lite": "^1.0.30001164",
    "color-convert": "^2.0.1",
    "cssesc": "^3.0.0",
    "d3-color": "^1.4.1",
    "dependency-graph": "^0.9.0",
    "enhanced-resolve": "^5.3.2",
    "escape-string-regexp": "^4.0.0",
    "esm": "^3.2.25",
    "fastest-levenshtein": "^1.0.12",
    "khroma": "^1.1.0",
    "lodash": "^4.17.20",
    "postcss": "^8.1.10",
    "postcss-scss": "^3.0.4",
    "postcss-selector-parser": "^6.0.4",
    "postcss-value-parser": "^4.1.0",
    "postcss-values-parser": "^4.0.0",
    "resolve": "^1.19.0",
    "resolve-from": "^5.0.0",
    "synchronous-promise": "^2.0.13",
    "tailwindcss": "^2.1.1",
    "ts-node": "^9.0.0",
    "unique-slug": "^2.0.2",
    "yargs": "^16.1.1"
  }
}
